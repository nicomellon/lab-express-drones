<h2>Available drones</h2>
{{#each dronesInDb}}
  <h3>{{name}}</h3>
  <ul>
    <li>Propellers: {{propellers}}</li>
    <li>Max Speed: {{maxSpeed}}</li>
  </ul>
  <a href="/drones/{{_id}}/edit">Edit</a>
  <form method="POST" action="/drones/{{_id}}/delete">
    <button type="submit">Delete</button>
  </form>
{{/each}}